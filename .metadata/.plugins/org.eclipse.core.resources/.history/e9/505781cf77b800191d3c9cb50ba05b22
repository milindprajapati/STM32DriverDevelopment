#ifndef INC_STM32F302XX_H_
#define INC_STM32F302XX_H_


/* Base Address of Memory */

#define FLASH_BASEADDR 		0x08000000U
#define SRAM_BASEADDR  		0x20000000U
#define ROM_BASEADDR		0x1FFFD800U


#endif /* INC_STM32F302XX_H_ */
